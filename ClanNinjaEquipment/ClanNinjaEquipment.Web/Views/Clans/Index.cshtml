@model IEnumerable<ClanNinjaEquipment.DataLayer.Clan>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SymbolPic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsEvil)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsDeleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedOn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClanGUID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SymbolPic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsEvil)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsDeleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedOn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClanGUID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ClanID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ClanID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ClanID })
        </td>
    </tr>
}

</table>
